CC = gcc
CFLAGS = -Wall -ansi -pedantic -O2
NONANSI_CFLAGS = -O2

OBJS = Tabs.o ComboBox.o ListTree.o XgUtils.o x_rotate.o	

X_INCLUDE = -I/usr/openwin/include -I/usr/dt/include
INCLUDE = -I../include

ALL_INCLUDES = $(INCLUDE) $(X_INCLUDE) 

# location of the libXb files
LIBXBDIR = ./Xb-1.0

all: objs libs

objs: $(OBJS)
	cd $(LIBXBDIR) ; $(MAKE)

libs: libxqs.a

libxqs.a: $(OBJS)
	$(AR) $(ARFLAGS) $@ $?

%.o: %.c
	$(CC) -c $< $(ALL_INCLUDES) -o $@

$(LIBXBDIR)/libXb.a:
	cd $(LIBXBDIR) ; make

XgUtils.o:
	 $(CC) -c $< -D USE_XM_MWMUTIL_H $(NONANSI_CFLAGS) $(ALL_INCLUDES) -o $@

clean:
	rm -f core `find . -name '*.[oas]' -print`
	rm -f libxqs.a

